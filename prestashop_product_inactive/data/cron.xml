<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record forcecreate="True" id="ir_cron_inactive_products" model="ir.cron">
        <field name="name">PrestaShop - Inactive deleted Products and Combinations</field>
        <field name="active" eval="False"/>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">1</field>
        <field name="state">code</field>
        <field name="code">model._scheduler_inactive_deleted_products()</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
        <field name="model_id" ref="connector_prestashop.model_prestashop_backend"/>
    </record>

</odoo>
